{
  "name": "stash-pull-request",
  "version": "1.0.0",
  "description": "Create a pull request easily in Stash",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "ts-node node_modules/jest/bin/jest ./src/*spec.ts --passWithNoTests",
    "build": "npm test && tsc",
    "compile": "tsc",
    "tsc:watch": "tsc --watch",
    "docs": "typedoc --out docs src",
    "lint": "tslint -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcorral/stash-pull-request.git"
  },
  "keywords": [
    "stash",
    "atlassian",
    "pull-request"
  ],
  "author": "Tomas Corral",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tcorral/stash-pull-request/issues"
  },
  "homepage": "https://github.com/tcorral/stash-pull-request#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "jest": "^27.5.1",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "jest": {
    "verbose": true
  }
}
